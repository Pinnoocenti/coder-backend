<h1>Los productos agregados hasta el momento son:</h1>

<div>
    {{#each data.payload}}
    <div>
        <h3>Producto: {{this.title}}</h3>
        <p>Descripción: {{this.description}}</p>
        <p>Precio: {{this.price}}</p>
        <p>Código: {{this.code}}</p>
        <p>Stock: {{this.stock}}</p>
        <p>Categoría: {{this.category}}</p>
        <p>Status: {{this.status}}</p>
        <p>Habilitado: {{this.available}}</p>
    </div>
    <br>
    <br>
    {{/each}}
    {{#if data.hasPrevPage}}
        <button><a href={{data.prevLink}}>Página anterior</a></button>
    {{/if}}

    <span>Página: {{data.page}}</span>

    {{#if data.hasNextPage}}
        <button><a href={{data.nextLink}}>Página siguiente</a></button>
    {{/if}}
    
</div>


<script src="js/index.js"></script>